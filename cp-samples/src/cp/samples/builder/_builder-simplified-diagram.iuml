@startuml
class PersonBuilder {
    -personAttributes
    +<<constructor>> PersonBuilder(name)
    +withSurname(surname): PersonBuilder
    +withAddress(address): PersonBuilder
    +withEmail(email): PersonBuilder
    +build(): Person
}

class Person {
    -name: String
    -surname: String
    -address: String
    -email: String
    -<<constructor>> Person(builder)
    +publicGetters()
}

PersonBuilder --> Person
@enduml
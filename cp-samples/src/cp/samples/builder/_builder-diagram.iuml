@startuml
class Person {
    -name: String
    -surname: String
    -address: String
    -email: String
    +Person(personBuilder)
    //getters
}

static class PersonBuilder {
    //person attributes
    +PersonBuilder(name, surname)
    +withAddress(address): PersonBuilder
    +withEmail(email): PersonBuilder
    +build(): Person
}

Person.Builder --> Person
@enduml